ay, setStockHay] = useState(true)\n    const productos = db.Ropa\n    // const carrito = db.Carrito.productos\n\n    useEffect(() => {\n        productos.map((producto) => {\n            if (producto.id === idProducto) {\n                //console.log(\"entre soy igual\")\n                setPrenda(producto)\n            }\n        })\n        //console.log(\"soy lap renda\", prenda)\n    }, []);\n\n    const handleSuma = () => {\n        if (number + 1 <= prenda.stock) {\n            setNumber(number + 1)\n        }\n    }\n\n    const handleResta = () => {\n        if (number - 1 >= 0) {\n            setNumber(number - 1)\n        }\n    }\n\n/*      const addCarrito = () => {\n        carrito.push([idProducto, number])\n        //console.log(\"Este es el carrito ------>\", carrito)\n    } */\n \n    return (\n        <div className=\"container\" >\n            <div style={{ width: \"50%\"}}>\n                <h2> {prenda.nombre}</h2>\n                <p> Precio: {prenda.precio}$</p>\n                <p> Stock: {prenda.stock}</p>\n                <p> Descripcion: {prenda.descripcion}$</p>\n                <br />\n                <div>\n                    <p> {number} </p>\n                    <button type=\"button\" onClick={handleResta} disabled={!stockHay}> minus </button>\n                    <button type=\"button\" onClick={handleSuma} disabled={!stockHay}> plus  </button>\n                </div>\n                <button type=\"button\" disabled={number === 0} onClick={() => {addItemToCart(prenda, number)}} > agregar al carrito </button>\n            </div>\n            <div style={{ width: \"50%\"}}>\n                <img\n                    className=\"remera\"\n                    src={prenda.imagen}\n                />\n            </div>\n        </div>\n    )\n}\nexport default ItemDetail;",[],"/home/agustin/Desktop/Proyectos/ReactBrand/src/Components/Carrito/Carrito.jsx",["89","90"],[],"/home/agustin/Desktop/Proyectos/ReactBrand/src/Components/Carrito